---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Henrik Tudborg</title>
    <link href="http://tudb.org/atom.xml" rel="self"/>
    <link href="http://tudb.org"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://henriktudborg.dk</id>
    <author>
        <name>Henrik Tudborg</name>
        <email>henrik@tudb.org</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://henrktudborg.dk{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://henrktudborg.dk{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>